<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="SuperTextView">
        <attr name="super_text" format="string" />
        <attr name="super_text_color" format="color" />
        <attr name="super_text_size" format="dimension" />
    </declare-styleable>
    <declare-styleable name="MyTextView">
        <attr name="my_text" format="string|reference" />
        <attr name="text_color" format="color|reference" />
    </declare-styleable>
    <!--QQ计步器自定义属性-->
    <declare-styleable name="StepView">
        <attr name="border_width" format="dimension|reference" />
        <attr name="outer_border_color" format="color|reference" />
        <attr name="inner_border_color" format="color|reference" />
        <attr name="max_step" format="integer|reference" />
        <attr name="current_step" format="integer|reference" />
        <attr name="text" format="string|reference" />
        <attr name="textColor" format="color|reference" />
        <attr name="textSize" format="dimension|reference" />
    </declare-styleable>

    <declare-styleable name="ShapeView">
        <attr name="sv_border_width" format="dimension|reference" />
        <attr name="border_color" format="color|reference" />
        <attr name="gradient_color" format="color|reference" />
        <!--CIRCLE, TRIANGLE, RECTANGLE, SQUARE, PARALLELOGRAM-->
        <attr name="shape_type">
            <enum name="round" value="0" />
            <enum name="triangle" value="1" />
            <enum name="rectangle" value="2" />
            <enum name="square" value="3" />
            <enum name="parallelogram" value="4" />
        </attr>
        <attr name="paint_style">
            <enum name="fill" value="0" />
            <enum name="stroke" value="1" />
        </attr>
    </declare-styleable>


</resources>